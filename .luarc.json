{
  "runtime.version": "Lua 5.4",
  "runtime.path": [
    "scripts/?.lua"
  ],
  "runtime.pathStrict": true,
  "workspace.library": [
    "generated/**/*.lua"
  ],
  "workspace.ignoreDir": [
    ".github",
    "crates",
    "target"
  ],
  "workspace.ignoreSubmodules": false,
  "diagnostics.disable": [],
  "diagnostics.neededFileStatus": {
    "codestyle-check": "Any",
    "name-style-check": "Any",
    "unused-local": "None!"
  },
  "diagnostics.ignoredFiles": "Disable",
  "diagnostics.libraryFiles": "Disable"
}
