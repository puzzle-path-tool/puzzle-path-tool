{
  "runtime.version": "Lua 5.4",
  "runtime.path": [
    "crates/puzzle_path_tool/scripts/?.lua"
  ],
  "runtime.pathStrict": true,
  "workspace.library": [
    "generated/**/*.lua"
  ],
  "workspace.ignoreDir": [
    ".github",
    "target"
  ],
  "nameStyle.config": {
    "global_variable_name_style": [
      "pascal_case",
      "upper_snake_case"
    ]
  },
  "workspace.ignoreSubmodules": false,
  "diagnostics.disable": [],
  "diagnostics.neededFileStatus": {
    "codestyle-check": "Any",
    "name-style-check": "Any",
    "unused-local": "None!"
  },
  "diagnostics.ignoredFiles": "Disable",
  "diagnostics.libraryFiles": "Disable"
}
